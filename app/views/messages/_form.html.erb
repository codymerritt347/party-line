<%= form_for(@message) do |f| %>
  <%= f.label :content, "Message Content"%><br>
  <%= f.text_field :content %><br>

  <%= f.label :party_id, "Party ID" %><br>
  <%= f.number_field :party_id %><br>

  <%= f.label :user_id, "User ID" %><br>
  <%= f.number_field :user_id %><br>

  <%= f.submit %>
<% end %>