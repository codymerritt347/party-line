<%= form_for(@user) do |f| %>
  <label>First Name</label><br>
  <%= f.text_field :first_name %><br>

  <label>Last Name</label><br>
  <%= f.text_field :last_name %><br>

  <label>Email</label><br>
  <%= f.text_field :email %><br>

  <label>Password</label><br>
  <%= f.text_field :password %><br>

  <%= f.submit %>
<% end %>