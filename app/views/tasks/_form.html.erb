<%= form_for(@task) do |f| %>
  <%= f.label :title, "Task Title" %><br>
  <%= f.text_field :title %><br>

  <%= f.label :description, "Task Description" %><br>
  <%= f.text_area :description %><br>

  <%= f.label :party_id, "Party ID" %><br>
  <%= f.number_field :party_id %><br>

  <%= f.submit %>
<% end %>